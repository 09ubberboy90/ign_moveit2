            </link>
        </model>

        <model name="pyramid_box6">
            <pose>1.5 0 0.125 0 0 0</pose>
            <link name="pyramid_box6_link">
                <inertial>
                    <inertia>
                        <ixx>0.00005</ixx>
                        <ixy>0.0</ixy>
                        <ixz>0.0</ixz>
                        <iyy>0.00005</iyy>
                        <iyz>0.0</iyz>
                        <izz>0.00005</izz>
                    </inertia>
                    <mass>0.12</mass>
                </inertial>
                <collision name="pyramid_box6_collision">
                    <geometry>
